{
	"name": "svelte-type-checker-vscode",
	"description": "Check types on your svelte templates",
	"author": "David Pershouse",
	"license": "MIT",
	"version": "0.1.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/halfnelson/svelte-type-checker-vscode"
	},
	"keywords": [
		"svelte",
		"typescript",
		"vscode"
	],
	"engines": {
		"vscode": "^1.33.0"
	},
	"activationEvents": [
		"onLanguage:svelte"
	],
	"main": "./client/out/extension",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Svelte Type Checker",
			"properties": {}
		}
	},
	"scripts": {
		"vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
		"compile": "tsc -b",
		"watch": "tsc -b -w",
		"postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
		"test": "sh ./scripts/e2e.sh"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"tslint": "^5.16.0",
		"typescript": "^3.6.4"
	}
}
